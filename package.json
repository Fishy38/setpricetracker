{
  "name": "setpricetracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --config ./lib/rakuten/prisma.config.ts && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate --config ./lib/rakuten/prisma.config.ts",

    "prisma:generate": "prisma generate --config ./lib/rakuten/prisma.config.ts",
    "prisma:studio": "prisma studio --config ./lib/rakuten/prisma.config.ts",
    "prisma:push": "prisma db push --config ./lib/rakuten/prisma.config.ts",
    "prisma:migrate": "prisma migrate dev --config ./lib/rakuten/prisma.config.ts",
    "prisma:seed": "prisma db seed --config ./lib/rakuten/prisma.config.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "dotenv": "^17.2.3",
    "next": "16.0.10",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recharts": "^1.8.29",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.23",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}